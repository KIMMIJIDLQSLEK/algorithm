def solution(prices):
    result=[]
    for i in range(len(prices)-1):
        flag=0
        for j in range(i+1,len(prices)):
            if prices[i]>prices[j]:
                result.append(j-i)
                flag=1
                break;
        if flag==1:
            continue;
        result.append(len(prices)-1-i)

    result.append(0)
    return result
